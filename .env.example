# EmailJS Configuration
# Get these from https://dashboard.emailjs.com/admin/account
VITE_EMAILJS_PUBLIC_KEY=your_public_key_here
VITE_EMAILJS_SERVICE_ID=your_service_id_here
VITE_EMAILJS_TEMPLATE_ID=your_template_id_here

# GitHub OAuth for Decap CMS (Vercel only)
# Get these from https://github.com/settings/developers
# Not needed if using Netlify with Git Gateway
OAUTH_GITHUB_CLIENT_ID=your_github_oauth_client_id_here
OAUTH_GITHUB_CLIENT_SECRET=your_github_oauth_client_secret_here
